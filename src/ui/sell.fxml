<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<SplitPane xmlns:fx="http://javafx.com/fxml/1" dividerPositions="0.29797979797979796" minWidth="500.0"
           prefHeight="800.0"
           prefWidth="1100.0" xmlns="http://javafx.com/javafx/11"
           fx:controller="controllers.SellController">
    <AnchorPane maxWidth="310.0" minHeight="0.0" minWidth="300.0" prefHeight="160.0" prefWidth="100.0">
        <VBox prefHeight="798.0" prefWidth="324.0" style="-fx-background-color: #FFFFFF;" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Button mnemonicParsing="false" onAction="#onClickAdd" prefHeight="50.0" prefWidth="300.0"
                    style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Добавление">
                <font>
                    <Font name="Book Antiqua" size="24.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onClickEdit" prefHeight="50.0" prefWidth="300.0"
                    style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Редактирование">
                <font>
                    <Font name="Book Antiqua" size="24.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onClickDelete" prefHeight="50.0" prefWidth="300.0"
                    style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Удаление">
                <font>
                    <Font name="Book Antiqua" size="24.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="300.0"
                    style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Продажа">
                <font>
                    <Font name="Book Antiqua" size="24.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onClickWriteOff" prefHeight="50.0" prefWidth="300.0"
                    style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Списание">
                <font>
                    <Font name="Book Antiqua" size="24.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onClickReserve" prefHeight="50.0" prefWidth="300.0"
                    style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Резервирование">
                <font>
                    <Font name="Book Antiqua" size="24.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onClickDiscounts" prefHeight="50.0" prefWidth="300.0"
                    style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Акции">
                <font>
                    <Font name="Book Antiqua" size="24.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onClickSearch" prefHeight="50.0" prefWidth="300.0"
                    style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Поиск">
                <font>
                    <Font name="Book Antiqua" size="24.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onClickReports" prefHeight="50.0" prefWidth="300.0"
                    style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Отчеты">
                <font>
                    <Font name="Book Antiqua" size="24.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
        </VBox>
    </AnchorPane>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0"
                style="-fx-background-color: #FFFFFF;">
        <VBox prefHeight="798.0" prefWidth="782.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <VBox>
                <HBox alignment="CENTER">
                    <Label text="Продажа">
                        <font>
                            <Font name="Book Antiqua" size="64.0"/>
                        </font>
                    </Label>
                </HBox>
                <HBox alignment="CENTER_LEFT">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <Label prefHeight="39.0" prefWidth="136.0" text="Комикс">
                        <HBox.margin>
                            <Insets left="10.0" right="10.0"/>
                        </HBox.margin>
                        <font>
                            <Font name="Book Antiqua" size="24.0"/>
                        </font>
                    </Label>
                    <TextField fx:id="editTextComicName" prefHeight="39.0" prefWidth="344.0">
                        <HBox.margin>
                            <Insets right="10.0"/>
                        </HBox.margin>
                        <font>
                            <Font size="18.0"/>
                        </font>
                    </TextField>
                    <Button mnemonicParsing="false" onAction="#onClickAddInCart" prefHeight="39.0" prefWidth="216.0"
                            style="-fx-background-color: #ffffff; -fx-border-style: solid;" text="Добавить комикс">
                        <font>
                            <Font name="Book Antiqua" size="18.0"/>
                        </font>
                        <HBox.margin>
                            <Insets right="10.0"/>
                        </HBox.margin>
                    </Button>
                    <Button mnemonicParsing="false" onAction="#onClickReserved" prefHeight="39.0" prefWidth="175.0"
                            style="-fx-background-color: #ffffff; -fx-border-style: solid;" text="Отложенные">
                        <font>
                            <Font name="Book Antiqua" size="18.0"/>
                        </font>
                        <HBox.margin>
                            <Insets right="5.0"/>
                        </HBox.margin>
                    </Button>
                </HBox>
            </VBox>
            <AnchorPane prefHeight="733.0" prefWidth="782.0">
                <TableView fx:id="tableResult" style="-fx-font-size: 18px" prefHeight="553.0" prefWidth="782.0" AnchorPane.bottomAnchor="0.0"
                           AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </AnchorPane>
            <HBox alignment="CENTER">
                <Label prefHeight="105.0" prefWidth="260.0" text="Сумма">
                    <font>
                        <Font size="72.0"/>
                    </font>
                    <HBox.margin>
                        <Insets left="20.0"/>
                    </HBox.margin>
                </Label>
                <Label fx:id="labelAmount" prefHeight="105.0" prefWidth="276.0" text="0.0">
                    <font>
                        <Font size="72.0"/>
                    </font>
                    <HBox.margin>
                        <Insets left="20.0" right="20.0"/>
                    </HBox.margin>
                </Label>
                <Button mnemonicParsing="false" onAction="#onClickOk" prefHeight="83.0" prefWidth="274.0"
                        style="-fx-background-color: #FFFFFF; -fx-border-style: solid;" text="Продать комиксы">
                    <font>
                        <Font name="Book Antiqua" size="24.0"/>
                    </font>
                </Button>
            </HBox>
        </VBox>
    </AnchorPane>
</SplitPane>
